{

  "manifest_version": 2,

  "name": "Foursquare Venue Selector",
  "description": "Foursquare Venue Selector",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    { "matches": ["http://www.flickr.com/photos/*"], "js": ["content_script.js"] }
  ],

  "permissions": ["tabs"],

  "page_action" : {
    "default_icon" : "img/icon16.png",
    "default_title" : "There's a flickr in this URL!",
    "default_popup": "popup.html"
  },

  "icons" : {
    "48" : "img/icon48.png",
    "128" : "img/icon128.png"
  }

}
